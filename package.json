{
  "name": "notion-to-md",
  "version": "4.0.0-alpha.3",
  "engines": {
    "node": ">=12"
  },
  "main": "./build",
  "types": "./build/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "prebuild": "npm run clean",
    "clean": "rm -rf ./build",
    "test": "jest --coverage --verbose",
    "test:watch": "jest -w",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "@notionhq/client": "^2.0.0"
  },
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*/index.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "notion",
    "markdown",
    "md",
    "notion",
    "notion-api",
    "notion-to-md",
    "notion2md",
    "notion-export",
    "notion-exporter",
    "notion-convert",
    "export"
  ],
  "author": "Souvik Kar Mahapatra <souvikat001@gmail.com>",
  "license": "ISC",
  "files": [
    "build/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/souvikinator/notion-to-md.git"
  },
  "bugs": {
    "url": "https://github.com/souvikinator/notion-to-md/issues"
  },
  "homepage": "https://github.com/souvikinator/notion-to-md#readme",
  "description": "Convert notion pages, block and list of blocks to markdown, html, jsx, latex, pdf and literally anything and export anywhere, compatible with any content workflow",
  "devDependencies": {
    "@notionhq/client": "^2.0.0",
    "@types/download": "^8.0.5",
    "@types/jest": "^27.4.1",
    "@types/mime": "^3.0.0",
    "@types/mime-types": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^27.4.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "mime": "^3.0.0",
    "node-fetch": "^2.7.0"
  }
}
